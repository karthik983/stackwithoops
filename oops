function Person(firstName,lastName,age,gender,interests){
    this.firstName=firstName;
    this.lastName=lastName;
    this.age=age;
    this.gender=gender;
    this.interests=interests;
    this.bio=()=>{ 
        console.log(this.firstName+this.lastName);
    }
}
function Teacher(firstName,lastName,age,gender,interests,subject){
    //Prototypal Inheritance
    // Person.call(this,firstName,lastName,age,gender,interests);
    this.subject=subject;
}
// Person.prototype.bio=()=>{
//     console.log(this.firstName);
// }
// Person.prototype.address="Someplacenice"
const karthi=new Teacher("Karthik","S",22,"M",["code","movie"],"Science");
// const karthi2=new Teacher("Karthik","S",22,"M",["code","movie"],"Math");
console.log(karthi)
Teacher.prototype = Object.create(Person.prototype);

